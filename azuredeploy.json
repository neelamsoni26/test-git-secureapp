{
  "name": "test-github-Appservice",
  "type": "Microsoft.Web/sites",
  "apiVersion": "2021-02-01",
  "location": "Central US",
  "properties": {
    "siteConfig": {
      "appSettings": [
        {
          "name": "WEBSITE_RUN_FROM_PACKAGE",
          "value": "1"
        }
      ]
    }
  },
  "resources": [
    {
      "name": "ZipDeploy",
      "type": "Extensions",
      "apiVersion": "2021-02-01",
      "dependsOn": [
        "[concat('Microsoft.Web/sites/', 'test-github-Appservic')]"
      ],
      "properties": {
        "packageUri": "release.zip"
      }
    }
  ]
}
